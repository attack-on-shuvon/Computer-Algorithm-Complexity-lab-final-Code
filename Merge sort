


#include<bits/stdc++.h>
using namespace std;
int num[100];
int c[]={0};

void merging(int l,int mid,int h){
    int i=l,j=mid+1,k=l;
    while(i<=mid && j<=h){
       if(num[i]<num[j]){
        c[k]=num[i];
        i++;
        k++;

       }
       else{
        c[k]=num[j];
        j++;
        k++;
       }
    }
    for(; i<=mid;i++){
        c[k]=num[i];
        k++;
    }
    for(; j<=h;j++){
        c[k]=num[j];
        k++;
    }

  for(int i=l;i<k;i++){
    num[i]=c[i];
  }


}

void mergesort(int l,int h){
    if(l<h){
            int mid=(l+h)/2;
        mergesort(l,mid);
        mergesort(mid+1,h);
        merging(l,mid,h);
    }

}

int main(){

    int n;cin>>n;
    for(int i=0;i<n;i++){
        cin>>num[i];
    }
    mergesort(0,n-1);
    for(int i=0;i<n;i++){
        cout<<num[i];
    }




}




